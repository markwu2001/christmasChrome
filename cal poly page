      up.jQuery(".portal-navigation").click(function(event){
        if (typeof newrelic == 'object') {
          var tabId = event.currentTarget.id;
          var tabName = event.target.innerText;
          var idArr = tabId.split("portalNavigation_");
          var type = "Unknown";

          if (idArr.length > 1){
            // Default portal tab IDs look like: portalNavigation_u18l1s6, custom: portalNavigation_s21
            type = idArr[1].charAt(0) === 'u' ? "Default" : "Custom"
          }
          newrelic.addPageAction('clicked_portal_tab', {title: tabName, type: type});
        }
      });
